{
  "name": "cantautor.es",
  "version": "1.0.2",
  "description": "Eleventy starter built around the workflow suggested by Andy Bell's buildexcellentwebsit.es.",
  "author": "Lene Saile",
  "type": "module",
  "engines": {
    "node": ">=20.x.x"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev:colors": "node ./src/_config/utils/create-colors.js",
    "dev:11ty": "eleventy --serve --watch",
    "build:colors": "node ./src/_config/utils/create-colors.js",
    "build:11ty": "eleventy",
    "start": "run-p dev:*",
    "build": "run-s clean build:*",
    "spotify:generate": "node ./src/_config/spotify/fetch-spotify-data.js",
    "spotify:final": "node ./src/_config/spotify/create-final-artists-array.js",
    "spotify": "run-s spotify:*"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/madrilene/cantautor.es.git"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.1.0",
    "@11ty/eleventy-fetch": "^5.1.0",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-plugin-inclusive-language": "^1.0.3",
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.1",
    "@11ty/eleventy-plugin-webc": "^0.11.2",
    "@11ty/is-land": "^4.0.1",
    "colorjs.io": "^0.5.2",
    "lite-youtube-embed": "^0.3.3",
    "sharp": "^0.34.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@toycode/markdown-it-class": "^1.2.4",
    "autoprefixer": "^10.4.21",
    "clean-css": "^5.3.3",
    "cssnano": "^7.0.7",
    "dayjs": "^1.11.13",
    "dotenv": "^16.5.0",
    "esbuild": "^0.25.4",
    "html-minifier-terser": "^7.2.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "markdown-it": "^14.1.0",
    "markdown-it-abbr": "^2.0.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-eleventy-img": "^0.10.2",
    "markdown-it-emoji": "^3.0.0",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-mark": "^4.0.0",
    "markdown-it-prism": "^3.0.0",
    "netlify-plugin-cache": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.0",
    "postcss-import-ext-glob": "^2.1.1",
    "postcss-js": "^4.0.1",
    "prettier-plugin-jinja-template": "^2.1.0",
    "rgb-hex": "^4.1.0",
    "rimraf": "^6.0.1",
    "sanitize-html": "^2.17.0",
    "sharp-ico": "^0.1.5",
    "slugify": "^1.6.6",
    "svgo": "^3.3.2"
  }
}
